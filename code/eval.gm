let
  fun mult x y = x * y
  val multBy11 = mult 11

  fun arithmetic a (b, c) {d, e} = (a / b + c - d) * e

  fun testRef () =
    let
      val x = ref 0
    in
      (x := 10; $x)
    end

  fun fact x = if x
               then x * fact(x - 1)
               else 1

  fun sum m n = if m
                then m + n + sum (m - 1) (n - 1)
                else 0

  fun f x = x
  fun g a = f a

  fun fib n = if (n = 0) orelse (n = 1) orelse (n = 2)
              then 1
              else fib(n - 1) + fib(n - 2)

