structure A =
struct

    (* int ops *)
    val _ = (~x;
             x + y;
             x * y;
             x - y;
             x / y;
             x % y;
             x + (y - z);
             x + (y * z);
             ((x % z) * y) - (~x)
            )

    (* bit ops *)
    val _ = (b';
             b & c;
             b | c;
             b ^ c;
             b << x;
             b >> x;
             b >>> x;
             b >>> x + y;
             b & c | d;
             (b | c)')

    (*  *)
    val _
end
